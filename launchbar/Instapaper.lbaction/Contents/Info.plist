<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>CFBundleIdentifier</key>
	<string>com.renaghan.launchbar.Instapaper</string>
	<key>CFBundleName</key>
	<string>Add to Instapaper</string>
	<key>CFBundleVersion</key>
	<string>2.0</string>
	<key>CFBundleIconFile</key>
	<string>instapaper.png</string>
	<key>LBDebugLogEnabled</key>
	<true/>
	<key>LBTextInputTitle</key>
	<string>URL</string>
	<key>LBScripts</key>
	<dict>
		<key>LBDefaultScript</key>
		<dict>
			<key>LBScriptName</key>
			<string>instapaper.js</string>
			<key>LBRequiresArgument</key>
			<false/>
			<key>LBAcceptedArgumentTypes</key>
			<array>
			  <string>string</string>
			</array>
			<key>LBAllowsInstantReuseOfLastTextInputString</key>
			<true/>
			<key>LBRunInBackground</key>
			<false/>
			<key>LBReturnsResult</key>
			<true/>
		</dict>
	</dict>
	<key>LBDescription</key>
	<dict>
		<key>LBSummary</key>
		<string>Adds input string URL, or passed in LaunchBar item, or URL from current tab in Safari to Instapaper. Returns the created item from Instapaper for further action chaining or opening. Execute with command-key (quick mode) to add to Instapaper and not return the URL item</string>
		<key>LBAuthor</key>
		<string>Padraic Renaghan</string>
		<key>LBEmail</key>
		<string>prenagha@renaghan.com</string>
		<key>LBWebsiteURL</key>
		<string>https://prenagha.github.io/launchbar/instapaper.html</string>
		<key>LBTwitter</key>
		<string>@prenagha</string>
	  <key>LBUpdateURL</key>
	  <string>https://raw.githubusercontent.com/prenagha/launchbar/master/Instapaper.lbaction/Contents/Info.plist</string>
	  <key>LBDownloadURL</key>
	  <string>https://s3.amazonaws.com/renaghan/lbdist/Instapaper.lbaction</string>
	  <key>LBChangelog</key>
	  <string>
	    2.0: Default from current tab in Safari
      1.2: Better javascript support
	    1.0: First release
	  </string>
	</dict>
</dict>
</plist>
